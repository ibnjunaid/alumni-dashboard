<div class="page-header">
  <%= link_to alumni_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_alumnus_path(@alumnus), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show alumnus</h1>
</div>

<dl class="dl-horizontal">
  <dt>Roll no:</dt>
  <dd><%= @alumnus.roll_no %></dd>

  <dt>Name:</dt>
  <dd><%= @alumnus.name %></dd>

  <dt>Grad year:</dt>
  <dd><%= @alumnus.grad_year %></dd>

  <dt>Email:</dt>
  <dd><%= @alumnus.email %></dd>

  <dt>College mail:</dt>
  <dd><%= @alumnus.college_mail %></dd>

  <dt>Phone no:</dt>
  <dd><%= @alumnus.phone_no %></dd>

  <dt>Company name:</dt>
  <dd><%= @alumnus.company_name %></dd>

  <dt>Designation:</dt>
  <dd><%= @alumnus.designation %></dd>

  <dt>Location:</dt>
  <dd><%= @alumnus.location %></dd>

  <dt>Linkedin:</dt>
  <dd><%= @alumnus.linkedIn %></dd>

  <dt>Facebook:</dt>
  <dd><%= @alumnus.facebook %></dd>

</dl>
