<p id="notice"><%= notice %></p>

<li>
<%= link_to('Logout', destroy_alumnus_session_path, method: :delete) %>
</li>

<h1>Events</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>

    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.name %></td>
        <%= print event.Eventpic.thumb.url %>
        <td><img src="<%= event.Eventpic.thumb.url%>" alt="No image found" /></td>
        <td><%= link_to 'Show', event %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
